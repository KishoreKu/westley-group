{
    "hosting": {
        "target": "westley-group-app",
        "public": ".",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**",
            "backend/**",
            "**/*.md",
            "**/*.sh"
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}